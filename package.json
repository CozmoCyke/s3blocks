{
  "name": "s3blocks",
  "version": "4.3.0",
  "description": "Render scratchblocks code to SVG images.",
  "main": "headless.js",
  "browser": "browser.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest",
    "test-headless": "node tests/headless.js",
    "build": "browserify browser.js -o build/scratchblocks.min.js -t [ babelify --presets [ es2015 babili ] ]",
    "fmt": "prettier --no-semi --trailing-comma es5 --write *.js lib/*.js",
    "start": "beefy browser.js:bundle.js --live --port 8080"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s3blocks/s3blocks.git"
  },
  "keywords": [
    "scratch"
  ],
  "author": "NitroCipher",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/s3blocks/s3blocks/issues"
  },
  "homepage": "https://s3blocks.github.io/",
  "devDependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "babili": "^0.0.11",
    "beefy": "^2.1.8",
    "browserify": "^14.1.0",
    "jest": "^19.0.2",
    "prettier": "1.7.4"
  },
  "dependencies": {
    "canvas": "1.4.0",
    "xmldom": "^0.1.22"
  }
}
